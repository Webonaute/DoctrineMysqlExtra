{
  "name": "webonaute/doctrine-mysql-extra",
  "type": "library",
  "description": "Some extra stuff for doctrine with MYSQL Plateform.",
  "keywords": [
    "doctrine",
    "symfony",
    "bundle",
    "symfony4",
    "mysql",
    "database",
    "extra"
  ],
  "homepage": "https://github.com/Webonaute/DoctrineMysqlExtra",
  "license": "MIT",
  "authors": [
    {
      "name": "Mathieu Delisle",
      "email": "mdelisle@webonaute.ca"
    }
  ],
  "require": {
    "php": ">=7.2",
    "doctrine/doctrine-bundle": "^1.8"
  },
  "require-dev": {
    "symfony/monolog-bundle": "^3.0",
    "symfony/console": "^4.1",
    "symfony/phpunit-bridge": "^4.1",
    "symfony/browser-kit": "^4.1",
    "symfony/profiler-pack": "^1.0",
    "symfony/twig-bundle": "^4.1",
    "phpunit/phpunit": "^6.0|^7.0",
    "dragonmantank/cron-expression": "^2.2"
  },
  "suggest": {
    "dragonmantank/cron-expression": "Require if you use CronExpressionType."
  },
  "autoload": {
    "psr-4": {
      "Webonaute\\DoctrineMysqlExtra\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": { "Tests\\Webonaute\\DoctrineMysqlExtra\\": "./tests/" }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  },
  "support": {
    "email": "mathieu@delisle.com",
    "issue": "https://github.com/Webonaute/DoctrineMysqlExtra/issues",
    "source": "https://github.com/Webonaute/DoctrineMysqlExtra"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
